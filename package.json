{
  "name": "nextjs-ssl",
  "version": "0.1.0",
  "description": "Serve secure NextJS",
  "main": "dist/bundles/bundle.umd.js",
  "module": "dist/bundles/bundle.esm.js",
  "type": "module",
  "exports": {
    "import": "./dist/esm/server.js",
    "require": "./dist/cjs/server.js"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --out-dir 'dist/esm' --source-maps",
    "build:cjs": "babel src --plugins @babel/plugin-transform-modules-commonjs --out-dir 'dist/cjs' --source-maps",
    "build:bundles": "rollup -c rollup.config.js"
  },
  "bin": {
    "next-ssl": "./src/cli.js"
  },
  "author": "RaphaÃ«l Joie",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "cross-env": "^7.0.3",
    "next": "^12.0.7",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.62.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
